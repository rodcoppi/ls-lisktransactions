# Local Development Environment Variables
# Copy this file to .env.local and update with your local values

# Next.js Configuration
NEXTAUTH_SECRET=local-dev-secret-key-change-in-production
NEXTAUTH_URL=http://localhost:3000

# Database Configuration (Local Development)
DATABASE_URL=postgresql://dashboard_user:dashboard_password@localhost:5432/dashboard_db

# Redis Configuration (Local Development)
REDIS_URL=redis://localhost:6379

# Development-specific settings
NODE_ENV=development
LOG_LEVEL=debug
ENABLE_DEBUGGER=true
ENABLE_HOT_RELOAD=true

# Local API Keys (Development only - use test keys)
LISK_API_KEY=dev-api-key
COINGECKO_API_KEY=dev-coingecko-key